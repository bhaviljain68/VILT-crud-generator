{
    "name": "artisanalbyte/vilt-crud-generator",
    "description": "Laravel Inertia CRUD Generator Package",
    "type": "library",
    "require": {
        "php": "^8.1",
        "laravel/framework": "^12.0",
        "inertiajs/inertia-laravel": "^0.6|^2.0",
