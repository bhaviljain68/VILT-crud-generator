{
    "name": "artisanalbyte/inertia-crud-generator",
    "description": "Laravel Inertia CRUD Generator Package",
    "type": "library",
    "require": {
        "php": "^8.1",
        "laravel/framework": "^10.0|^11.0|^12.0",
        "inertiajs/inertia-laravel": "^0.6|^2.0",
        "maatwebsite/excel": "^3.1",
        "barryvdh/laravel-dompdf": "^2.0 || dev-master",
        "doctrine/dbal": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "artisanalbyte\\InertiaCrudGenerator\\": "src/",
            "App\\": "app/" 
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "artisanalbyte\\InertiaCrudGenerator\\InertiaCrudGeneratorServiceProvider"
            ]
        }
    },
    "autoload-dev": {
        "psr-4": {
        }
    },
    "require-dev": {
        
    },
    "config": {
        "allow-plugins": {
        }
    }
}
