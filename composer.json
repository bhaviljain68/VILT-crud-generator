{
    "name": "artisanalbyte/inertia-crud-generator",
    "description": "Laravel Inertia CRUD Generator Package",
    "type": "library",
    "require": {
        "php": "^8.1",
        "laravel/framework": "^10.0",
        "inertiajs/inertia-laravel": "^0.6",
        "maatwebsite/excel": "^3.1",
        "barryvdh/laravel-dompdf": "^2.0",
        "doctrine/dbal": "^3.9",
        "carbonphp/carbon-doctrine-types": "^2.1"
    },
    "autoload": {
        "psr-4": {
            "artisanalbyte\\InertiaCrudGenerator\\": "src/",
            "App\\": "app/" 
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "artisanalbyte\\InertiaCrudGenerator\\InertiaCrudGeneratorServiceProvider"
            ]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "artisanalbyte\\InertiaCrudGenerator\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "orchestra/testbench": "^8.36",
        "pestphp/pest": "^2.36",
        "pestphp/pest-plugin-laravel": "^2.4",
        "phpunit/phpunit": "^10.5"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
